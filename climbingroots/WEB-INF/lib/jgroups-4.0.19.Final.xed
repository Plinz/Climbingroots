<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
<id>org.jgroups:jgroups</id>
<version>4.0.19.Final</version>
<type>jar</type>
<name>JGroups</name>
<summary>Reliable cluster communication toolkit</summary>
<website>http://www.jgroups.org</website>
<authors>
<author>
<name>Bela Ban</name>
</author>
</authors>
<licenses>
<license>
<name>Apache License 2.0</name>
</license>
</licenses>
<scm>
<url>https://github.com/belaban/JGroups</url>
<connection>
<system>git</system>
<path>git://github.com/belaban/JGroups.git</path>
</connection>
<developerconnection>
<system>git</system>
<path>ssh://git@github.com/belaban/JGroups.git</path>
</developerconnection>
</scm>
<issuemanagement>
<system>jira</system>
<url>https://jira.jboss.com/jira/browse/JGRP</url>
</issuemanagement>
<dependencies>
<dependency>
<id>org.apache.logging.log4j:log4j-core</id>
<version>[2.0,)</version>
<optional type="boolean">true</optional>
<properties>
<maven.dependency.scope>compile</maven.dependency.scope>
</properties>
</dependency>
<dependency>
<id>org.apache.logging.log4j:log4j-api</id>
<version>[2.0,)</version>
<optional type="boolean">true</optional>
<properties>
<maven.dependency.scope>compile</maven.dependency.scope>
</properties>
</dependency>
<dependency>
<id>org.slf4j:slf4j-api</id>
<version>[1.7,)</version>
<optional type="boolean">true</optional>
<properties>
<maven.dependency.scope>compile</maven.dependency.scope>
</properties>
</dependency>
</dependencies>
<properties>
<maven.groupid>org.jgroups</maven.groupid>
<maven.artifactid>jgroups</maven.artifactid>
</properties>
</extension>
